package first.modules;

public class Module {
	
	private String name;
	private int key;
	private boolean toggled;
	
	public Module(String name, int key) {
		this.name = name;
		this.key = key;
		this.toggled = false;
	}
	
	public void onEnable() {
		
	}
	
	public void onDIsable() {
		
	}
	
	public boolean isToggled() {
		return this.toggled;
	}
	
	public String getName() {
		return this.name;
	}
	
	public void setName() {
		this.name = name;
	}
	
	public void toggle() {
		this.toggled = !this.toggled;
	}
	
	public int getKey() {
		return this.key;
	}
	
	public void setKey(int k) {
		this.key = k;
	}

}
